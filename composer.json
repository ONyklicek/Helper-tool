  },
  "authors": [
    {
      "name": "Ondřej Nyklíček",
      "email": "ondrejnyklicek@icloud.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^10"
  }
}
